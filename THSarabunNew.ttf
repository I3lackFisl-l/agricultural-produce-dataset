				<script type="text/javascript">
var baseIndex = "https:\/\/awards.opdc.go.th\/";
$(function() {
	$('.lang_sec [name=change_lang]').on('change',function(){
		var lang = $(this).val();
		console.log(lang);
		$.post(baseIndex+'front/sysf/set_language/'+lang,function(dataJson){
			var data = JSON.parse(dataJson);
			if(data){
				window.location = data;
			}
		});
	});
	
	$('.dropdown-submenu').on('click',function(e){
		var dtoggle = $(e.target).data('toggle');
		if(dtoggle !== 'dropdown') return ;
		
		var childDM = $(this).children('.dropdown-menu');
		if(childDM.length){
			$(this).siblings().children('.dropdown-menu').css('display','none');
			if(childDM.is(':visible')) childDM.css('display','none');
			else childDM.css('display','block');
			return false;
		}
	});
	
	$('.nav-link').on('click',function(){
		$(this).next('.dropdown-menu').find('.dropdown-menu').css('display','none');
	});
});
function changTextSize(_size){
	$('body')
		.removeClass('text_size_s text_size_m text_size_l')
		.addClass('text_size_'+_size);
}
function logout() {
	if(confirm('ต้องการออกจากระบบหรือไม่?')){
		$.post(baseIndex+"sys_main/sys/logout", function() {
			//location.reload();
			window.location.assign(baseIndex);
		});
	}
}

function login() {
	var login_site = baseIndex+'login';
	window.location.assign(login_site);
}

function register(){
	var register_site = baseIndex+'register';
	window.location.assign(register_site);
}

function last_visited(){
	var dia = $('<div class="modal fade dialog" tabindex="-1" role="dialog">');
		var data = {};
		$.post("https://awards.opdc.go.th/content/get_last_visited", data, function (data) {
			dia.append(data);
			dia.modal('show');
		});
		dia
			.on('shown.bs.modal', function (e) {
				//MSYS.centerElement(dia); // ตรงนี้ทำให้ dialog อยู่กึ่งกลางของหน้าจอ
			})
			.on('hidden.bs.modal', function (e) {
				dia.remove();
			});
}

</script>
